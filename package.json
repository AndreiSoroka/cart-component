{
  "name": "cart-component",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build; run-p build-prerender build-jest build-storybook build-e2e",
    "build-jest": "jest --coverage-reporters=html --no-cache",
    "build-e2e": "npx playwright test",
    "build-storybook": "storybook build -o ./dist/storybook",
    "build-prerender": "node prerender.mjs",
    "type-check": "tsc --noEmit",
    "commit-check": "run-p type-check test:unit prettier lint stylelint",
    "stylelint": "npx stylelint **/*.scss",
    "stylelint:fix": "npx stylelint **/*.scss --fix",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "test": "run-s test:unit test:e2e",
    "test:unit": "jest",
    "test:e2e": "playwright test --reporter=list",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.34.0",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-vite": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/core": "^1.3.57",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "json5": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "prop-types": "^15.8.1",
    "sass": "^1.62.1",
    "storybook": "^7.0.12",
    "stylelint": "^15.6.2",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.2"
  }
}
